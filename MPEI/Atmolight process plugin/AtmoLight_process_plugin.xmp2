using MpeCore.Classes;
using MpeCore;
using System;
using System.Diagnostics;
using System.Windows.Forms;

public class Script
{
    public static void Main(PackageClass packageClass, ActionItem actionItem)
    {
//kill AtmoWin process to make sure the .exe file can be deleted/overwritten if needed
     Process[] pp = Process.GetProcessesByName("AtmoWinA");
     foreach (Process p in pp)
    {
        p.Kill();
    }
    }
}
